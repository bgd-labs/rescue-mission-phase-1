{
  "name": "rescue-mission-phase-1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "generate-json": "env-cmd ts-node ./scripts/generate-address-value-maps.ts",
    "generate-json-formatted": "env-cmd ts-node ./scripts/format.ts",
    "generate-tree": "ts-node ./scripts/generate-merkle-root.ts",
    "aaveToken:current": "env-cmd cast etherscan-source -d etherscan 0xC13eac3B4F9EED480045113B7af00F7B5655Ece8",
    "aaveToken:diff": "npm run aaveToken:current && sh generateDiffs.sh ./etherscan/AaveTokenV2/AaveTokenV2.sol ./src/contracts/AaveTokenV2.sol AaveTokenV2",
    "aaveStakeToken:current": "env-cmd cast etherscan-source -d etherscan 0xe42F02713aEC989132C1755117F768DBEa523d2f",
    "aaveStakeToken:diff": "npm run aaveStakeToken:current && sh generateDiffs.sh ./etherscan/StakedTokenV2Rev3/StakedTokenV2Rev3.sol ./src/contracts/StakedTokenV2Rev4.sol StakedTokenV2Rev4",
    "lendToAaveMigrator:current": "env-cmd cast etherscan-source -d etherscan 0x86241b6c526998582556F7C0342D8863b604B17b",
    "lendToAaveMigrator:diff": "npm run lendToAaveMigrator:current && sh generateDiffs.sh ./etherscan/LendToAaveMigrator//contracts/token/LendToAaveMigrator.sol ./src/contracts/LendToAaveMigrator.sol LendToAaveMigrator",
    "deploy:test": "env-cmd npm run deployTest",
    "deploy": "env-cmd npm run deploy:broadcast",
    "deployVerify": "env-cmd npm run deploy:verify",
    "verify": "env-cmd npm run verify:proposalLong",
    "deployTest": "forge script ./src/deploy/deploy.sol --fork-url $FORK_URL --private-key $PRIVATE_KEY --optimize --optimizer-runs 200 -vvvv",
    "deploy:broadcast": "forge script ./src/deploy/deploy.sol --private-key $PRIVATE_KEY --broadcast --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv",
    "deploy:verify": "forge script ./src/deploy/deploy.sol --private-key $PRIVATE_KEY --fork-url $FORK_URL --verify --optimize --optimizer-runs 200 -vvvv",
    "test:contracts": "forge test --fork-url $FORK_URL -vvvv",
    "verify:aaveToken": "forge verify-contract --compiler-version v0.7.5+commit.eb77ed08 0xc7fcbe266969dd1241b3905ef6aeddeb54393dec src/contracts/AaveTokenV2.sol:AaveTokenV2 --num-of-optimizations 200 --chain-id 5",
    "verify:stkAave": "forge verify-contract --constructor-args $(cast abi-encode 'constructor(address,address,uint256,uint256,address,address,uint128,string,string,uint8)' 0x7Fc66500c84A76Ad7e9c93437bFc5Ac33E2DDaE9 0x7Fc66500c84A76Ad7e9c93437bFc5Ac33E2DDaE9 864000 172800 0x25F2226B597E8F9514B3F68F00f494cF4f286491 0xEE56e2B3D491590B5b31738cC34d5232F378a8D5 3155692600 'Staked Aave' 'stkAAVE' 18) --compiler-version v0.7.5+commit.eb77ed08 0x8da84166835e6d81af1ad796e2ac64c1094563d7 src/contracts/StakedTokenV2Rev4.sol:StakedTokenV2Rev4 --num-of-optimizations 200 --chain-id 5",
    "verify:lendToAaveMigrator": "forge verify-contract --constructor-args $(cast abi-encode 'constructor(address,address,uint256)' 0x7Fc66500c84A76Ad7e9c93437bFc5Ac33E2DDaE9 0x80fB784B7eD66730e8b1DBd9820aFD29931aab03 100) --compiler-version v0.8.0+commit.c7dfd78e 0x3ca9a9b53f5fb7e8461fe742ed6ab7d99e3b4bbc src/contracts/LendToAaveMigrator.sol:LendToAaveMigrator --num-of-optimizations 200 --chain-id 5",
    "verify:proposalShort": "forge verify-contract --constructor-args $(cast abi-encode 'constructor(address,address)' 0x845de02a52d5a3c3e834e2dee77e0e2b09ad5221 0x3ca9a9b53f5fb7e8461fe742ed6ab7d99e3b4bbc) --compiler-version v0.8.0+commit.c7dfd78e 0xc86b1ece665d032ec6c7c13e5bcf994f039447b8 src/contracts/ProposalPayloadShort.sol:ProposalPayloadShort --num-of-optimizations 200 --chain-id 5",
    "verify:proposalLong": "forge verify-contract --constructor-args $(cast abi-encode 'constructor(address,address,address)' 0x845de02a52d5a3c3e834e2dee77e0e2b09ad5221 0xc7fcbe266969dd1241b3905ef6aeddeb54393dec 0x8da84166835e6d81af1ad796e2ac64c1094563d7) --compiler-version v0.7.5+commit.eb77ed08 0x52be01a12eb9f4297f30e6a2d3e7b37b196b00ea src/contracts/ProposalPayloadLong.sol:ProposalPayloadLong --num-of-optimizations 200 --chain-id 5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgd-labs/rescue-mission-phase-1.git"
  },
  "keywords": [],
  "author": "sendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bgd-labs/rescue-mission-phase-1/issues"
  },
  "homepage": "https://github.com/bgd-labs/rescue-mission-phase-1#readme",
  "devDependencies": {
    "@aave/contract-helpers": "^1.3.2",
    "@aave/math-utils": "^1.3.2",
    "@openzeppelin/contracts": "^4.6.0",
    "@supercharge/promise-pool": "^2.1.0",
    "@types/node": "^17.0.34",
    "bignumber.js": "^9.0.2",
    "env-cmd": "^10.1.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.6",
    "npm": "^8.10.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "prettier-plugin-solidity": "^1.0.0-beta.19"
  }
}
